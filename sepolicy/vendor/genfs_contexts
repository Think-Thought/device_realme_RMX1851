# Devinfo
genfscon proc /devinfo                                                     u:object_r:proc_devinfo:s0

# Fingerprint
genfscon sysfs /devices/platform/soc/soc:fpc_fpc1020/irq                   u:object_r:sysfs_fingerprint:s0
genfscon sysfs /devices/platform/soc/soc:fpc_fpc1020/irq_enable            u:object_r:sysfs_fingerprint:s0
genfscon sysfs /devices/platform/soc/soc:fpc_fpc1020/wakelock_enable       u:object_r:sysfs_fingerprint:s0
genfscon proc /fp_id                                                       u:object_r:fp_proc:s0

# Flash
genfscon proc /qcom_flash                                                  u:object_r:cam_proc:s0

# Graphics
genfscon sysfs /devices/platform/soc/soc:qcom,msm-ext-disp/extcon/extcon2/name       u:object_r:vendor_sysfs_graphics:s0
genfscon sysfs /devices/platform/soc/soc:qcom,msm-ext-disp/extcon/extcon3/name       u:object_r:vendor_sysfs_graphics:s0

# Project version
genfscon proc /oppoVersion                                                 u:object_r:oppo_ver_proc:s0

# Touchpanel
genfscon proc /touchpanel                                                  u:object_r:proc_touchpanel:s0

# Sched
genfscon proc /sys/kernel/sched_upmigrate                                  u:object_r:proc_sched:s0
genfscon proc /sys/kernel/sched_downmigrate                                u:object_r:proc_sched:s0
